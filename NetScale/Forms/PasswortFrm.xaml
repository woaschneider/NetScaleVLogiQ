<Window x:Class="HWB.NETSCALE.FRONTEND.WPF.Forms.PasswortFrm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Passwort"
        Width="242"
        Height="198"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Name="buttonOk"
                Width="75"
                Height="23"
                Margin="21,107,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="buttonOk_Click"
                Content="Ok" />
        <Label Name="label1"
               Width="116"
               Height="28"
               Margin="51,24,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               Content="Passwort eingeben:" />
        <Button Name="buttonCancel"
                Width="75"
                Height="23"
                Margin="111,107,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="buttonCancel_Click"
                Content="Abbrechen" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="51,69,0,0" Name="passwordBox1" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
